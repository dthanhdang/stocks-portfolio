{
  "name": "stocks-portfolio",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./client/components/financial_instrument_editor": "./src/client/components/financial_instrument_editor/index.ts",
    "./client/components/financial_instruments_list": "./src/client/components/financial_instruments_list/index.ts",
    "./client/components/text_link": "./src/client/components/text_link/index.ts",
    "./client/rpc": "./src/client/rpc/index.ts",
    "./client/tanstack/dev_tools/router": "./src/client/tanstack/dev_tools/router/index.ts",
    "./client/types": "./src/client/types/index.ts",
    "./server/conversions": "./src/server/conversions/index.ts",
    "./server/generated/db": "./src/server/_generated/DB.ts",
    "./server/queries": "./src/server/queries/index.ts",
    "./server/queries/financial_instrument": "./src/server/queries/financial_instrument/index.ts",
    "./server/routes": "./src/server/routes/index.ts"
  },
  "scripts": {
    "dev": "concurrently \"vite --config vite.config.server.ts\" \"vite --config vite.config.client.ts\"",
    "generate_code": "pnpm kysely:generate_types --camel-case && pnpm wrangler:generate_types && pnpm tsr:generate_routes",
    "kysely:generate_types": "pnpm kysely-codegen --camel-case --out-file ./src/server/_generated/DB.ts --url .wrangler/state/v3/d1/miniflare-D1DatabaseObject/4a075e2aacc5e6c94f6e7a67abf2b125a5d446b8aee21577b158e9983f64b334.sqlite",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "qa": "pnpm tsr generate && pnpm lint:check && pnpm prettier:check && pnpm tsc:check && pnpm unit_tests",
    "tsc:check": "tsc -noEmit -p tsconfig.app.json",
    "tsr:generate_routes": "tsr generate",
    "unit_tests": "pnpm vitest:client:run && pnpm vitest:server:run",
    "vite:build": "vite build --config vite.config.client.ts && vite build --config vite.config.server.ts",
    "vite:dev": "concurrently \"vite --config vite.config.server.ts\" \"vite --config vite.config.client.ts\"",
    "vitest:client:run": "vitest run --config=vitest.config.client.ts --mode development --passWithNoTests",
    "vitest:client:watch": "vitest watch --config=vitest.config.client.ts --mode development",
    "vitest:server:run": "vitest run --config=vitest.config.server.ts --mode development --passWithNoTests",
    "vitest:server:watch": "vitest watch --config=vitest.config.server.ts --mode development",
    "wrangler:apply_migrations": "pnpm wrangler d1 migrations apply development-stocks-portfolio --local",
    "wrangler:execute_sql": "wrangler d1 execute development-stocks-portfolio --local --command ",
    "wrangler:generate_types": "wrangler types --experimental-include-runtime=./src/server/_generated/wrangler.d.ts && rm -f worker-configuration.d.ts",
    "wrangler:preview": "wrangler pages dev",
    "wrangler:reset_db": "pnpm wrangler:execute_sql \"delete from d1_migrations\" && pnpm wrangler:apply_migrations && pnpm kysely:generate_types"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@hono/valibot-validator": "^0.5.2",
    "@hono/vite-cloudflare-pages": "^0.4.2",
    "@hono/vite-dev-server": "^0.17.0",
    "@mantine/carousel": "^7.15.1",
